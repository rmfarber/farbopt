echo "************ Don't forget to set the number of cores! **********"
echo "************ Currently set to " $OMP_NUM_THREADS
TIME=30 #seconds
NPRED=100
SIZE=60000000
rm -f train.dat param.dat
./genData.x - $SIZE 0.01 123 | numactl -m 1 ./nloptTrain.x - param.dat $TIME
./genData.x - $NPRED 0 123 | ./pred.x - param.dat  > pred.csv
