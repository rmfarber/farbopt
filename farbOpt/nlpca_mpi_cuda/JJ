APP=nlpca_mpi_cuda
NUM_NODES=2
EXAMPLES=30000
VAR=0.01

./gen_$APP $APP.train.dat $EXAMPLES $VAR 1234

mpiexec -np $NUM_NODES ./mpiTrain_$APP.cuda $APP.train.dat $APP.param $APP.timing.txt

./gen_$APP $APP.pred.dat 1000 0 1

./pred_$APP $APP.param $APP.pred.dat > output.txt

# create file for gnuplot
tail -n +3 output.txt > plotdata.txt

rm *.dat


