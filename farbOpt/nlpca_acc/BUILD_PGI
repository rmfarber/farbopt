. ./common.sh

FLAGS="-D$ACTIVATION -acc -w -O3 -use_fast_math -Minline=myFunc"

INC=$NLOPT_DIR/include
LIB=$NLOPT_DIR/lib

pgcc $GCC_FLAGS genData.c -o gen_$APP

pgcc -ta=nvidia,6.5,cc35 -Minfo=accel $FLAGS ../train.c $MAX_RUN -I . -I $INC  -L $LIB -lnlopt -lm -o train_$APP.acc

pgcc $GCC_FLAGS ../pred.c -I . -lm -o pred_$APP


